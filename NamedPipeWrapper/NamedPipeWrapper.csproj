<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	<PackageId>CloudVeil.NamedPipeWrapper</PackageId>
	<Version>1.5.2</Version>
	<Description>
	A simple, easy to use, strongly-typed wrapper around .NET named pipes.

This is a .NET Standard 2.0 build of the NamedPipeWrapper.
Original repo: https://github.com/acdvorak/named-pipe-wrapper
	</Description>
	<Authors>Andrew C. Dvorak</Authors>
	<Copyright>2013 Andrew C. Dvorak</Copyright>
	<PackageProjectUrl>https://github.com/cloudveiltech/named-pipe-wrapper</PackageProjectUrl>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.Pipes.AccessControl" Version="4.5.1" />
  </ItemGroup>
</Project>
